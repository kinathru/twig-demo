{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "outDir": "./public/js",
        "rootDir": "./src/assets/ts",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/assets/ts/**/*"],
    "exclude": ["node_modules"]
}
