{
    "name": "twig-demo-windsurf",
    "version": "1.0.0",
    "description": "A component-based website using Twig, SCSS, and TypeScript",
    "scripts": {
        "build:scss": "sass src/assets/scss:public/css",
        "build:ts": "tsc",
        "watch:scss": "sass --watch src/assets/scss:public/css",
        "watch:ts": "tsc --watch",
        "fractal:start": "fractal start --sync",
        "fractal:build": "fractal build"
    },
    "dependencies": {
        "bootstrap": "^5.3.2"
    },
    "devDependencies": {
        "@frctl/fractal": "^1.5.15",
        "@frctl/twig": "^1.2.13",
        "sass": "^1.69.5",
        "typescript": "^5.3.3"
    }
}
